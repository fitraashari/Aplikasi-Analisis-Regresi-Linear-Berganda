            <?php
function ysquare($kons, $kf1, $kf2, $kf3, $kf4, $kf5, $ttx1, $ttx2, $ttx3, $ttx4, $ttx5){
	$ujt  = $kons+($kf1*$ttx1)+($kf2*$ttx2)+($kf3*$ttx3)+($kf4*$ttx4)+($kf5*$ttx5);
	return $ujt;
}$sumy=mysqli_query($conn,"SELECT sum(total) as jumlahy from y");
$ttly=mysqli_fetch_assoc($sumy);
$qr_res=mysqli_query($conn,"SELECT count(responden) as totalres from y");
$totres=mysqli_fetch_assoc($qr_res);
 
$tampil=mysqli_query($conn,"SELECT x1.responden,x1.total as totx1,x2.total as totx2,x3.total as totx3,x4.total as totx4,x5.total as totx5,y.total as toty from x1,x2,x3,x4,x5,y where x2.responden = x1.responden and x3.responden = x1.responden and x4.responden = x1.responden and x5.responden = x1.responden and y.responden = x1.responden ");
$sumyy2 = 0;
$sumyy4 = 0;
while ($data=mysqli_fetch_assoc($tampil))
{
	$responden  = $data['responden'];
	$tres = $totres['totalres'];
	$jmly	= $ttly['jumlahy'];
    $totalx1  = $data['totx1'];
	$totalx2  = $data['totx2'];
	$totalx3  = $data['totx3'];
	$totalx4  = $data['totx4'];
	$totalx5  = $data['totx5'];
	$totaly	= $data['toty'];
	$ysqr	= ysquare($konstanta,$b1,$b2,$b3,$b4,$b5,$totalx1,$totalx2,$totalx3,$totalx4,$totalx5);
	$yy1	= $totaly-$ysqr;
	$yy2	= round($yy1*$yy1,3);
	$yrata	= round($jmly/$tres,3);
	$yy3	= round($ysqr-$yrata,3);
	$yy4	= round($yy3*$yy3,3);
	
	echo "<tr><td>$responden</td><td>$totaly</td><td>$ysqr</td><td>$yy1</td><td>$yy2</td><td>$yrata</td><td>$yy3</td><td>$yy4</td></tr>";
$sumyy2 = $yy2+$sumyy2;
$sumyy4 = $yy4+$sumyy4;
}
